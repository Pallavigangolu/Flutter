void main() {   
int a = 25;   
int b = 5;   
double c = 10.5;   
String d = 'Flutter'; 
bool e = true;   
int i; 
print('Int value is : $a');  
print('Float value is: $c');  
print('String value is: $d'); 
print('Boolean value is: $e'); 
   
 //Arithmetic Operators   
var add=a+b;   
var sub=a-b;  
var mul=a*b;   
var div=a/b; 
print("Sum of numbers is: $add");  
print("Difference of two numbers is: $sub");   
print("Multiplication of two numbers is: $mul"); 
print("Division of two numbers is: $div"); 
   
//Relational Operators   
print(a>b);  
print(a<b);  
print(a>=b);   
print(a<=b);   
print(a==b); 
print(a!=b); 
 
// Conditional statement 
if (a > 18) 
{     
  print('Person is eligible to vote'); 
}
else 
{ 
    print('Person is not eligible to vote');  
} 
   
//Looping Statement   
 for(i=1;i<=5;i++)
{    
    print("$i"); 
  } 
} 
